#include <iostream>
#include <cmath>
using namespace std;
double distance(double x,double a,double b,double y){

double d=pow((x-a),2);
double e=pow((y-b),2);
double j=d+e;
double dis=sqrt(j);
return dis;
}
int main(){

 double x,a,y,b;
    cout<<"enter the value of x1:"<<endl;
    cin>>x;
    cout<<"enter the value of x2:"<<endl;
    cin>>a;
    cout<<"enter the value of y1:"<<endl;
    cin>>y;
    cout<<"enter the value of y2:"<<endl;
    cin>>b;

    double dis=distance(x,a,b,y);
   cout<<"distance between point 1 and point 2 is :"<<dis<<endl;
return 0;
}
